{
  "name": "project",
  "dependencies": {
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1"
  },
  "devDependencies": {
    "vite": "latest"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite & npm run css:watch",
    "build": "npm run css:build && vite build",
    "css:build": "tailwindcss -i ./index.css -o ./tailwind.css --minify",
    "css:watch": "tailwindcss -i ./index.css -o ./tailwind.css --watch"
  }
}
